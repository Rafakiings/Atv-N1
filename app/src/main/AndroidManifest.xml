<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.atvn1">

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AtvN1">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Personagem"
            android:label="@string/label_Personagem">
        </activity>

        <activity
            android:name=".Anotacoes"
            android:label="@string/label_Anotacoes">
        </activity>

        <activity
            android:name=".PersonagemAdd"
            android:label="@string/label_PersonagemAdd">
        </activity>

        <activity
            android:name=".PersonagemInfo"
            android:label="@string/label_PersonagemInfo">
        </activity>

    </application>

</manifest>